	"dependencies": {
		"@eslint/js": "^9.19.0",
		"@next/eslint-plugin-next": "^15.1.6",
		"@vitest/eslint-plugin": "^1.1.25",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-compiler": "19.0.0-beta-decd7b8-20250118",
